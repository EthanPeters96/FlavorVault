{
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.github": true,
        "**/.gitp*": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/.vscode": true,
        "**/core.Microsoft*": true,
        "**/core.mongo*": true,
        "**/core.python*": true,
        "**/CVS": true
    },
    "files.autoSave": "off",
    "workbench.colorTheme": "Theme Darker",
    "terminal.integrated.gpuAcceleration": "canvas",
    "cSpell.words": [
        "Balsamiq",
        "bson",
        "carbonara",
        "Colour",
        "coolors",
        "dnspython",
        "Fontawesome",
        "Graeme",
        "Nescot",
        "Procfile",
        "pymongo",
        "signup",
        "sqlalchemy",
        "Usplash",
        "wireframes"
    ]
}
